{
  "name": "finca-bufalos-datave",
  "version": "1.0.0",
  "private": true,
  "description": "Sistema de gestión integral para granja de búfalos con Next.js y Supabase",
  "author": "Tu Nombre",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "dev:secure": "next dev --experimental-https",
    "build": "next build",
    "start": "next start",
    "start:prod": "NODE_ENV=production next start",
    "lint": "eslint . --ext .ts,.tsx --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "clean": "rm -rf .next node_modules",
    "clean:next": "rm -rf .next",
    "analyze": "ANALYZE=true next build",
    "postinstall": "npx next telemetry disable",
    "prepare": "husky install",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "db:generate": "npx supabase gen types typescript --project-id tu-project-id > types/supabase.ts",
    "db:seed": "ts-node scripts/seed.ts",
    "db:reset": "node scripts/reset-db.js",
    "migration:create": "npx supabase migration new",
    "migration:up": "npx supabase migration up",
    "validate": "npm run type-check && npm run lint && npm run test",
    "prepare-env": "node scripts/validate-env.js"
  },
  "dependencies": {
    "@ducanh2912/next-pwa": "^10.2.9",
    "@hookform/resolvers": "^4.1.3",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-toast": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.89.0",
    "@tanstack/react-query": "^5.18.0",
    "@tanstack/react-query-devtools": "^5.18.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "framer-motion": "^12.5.0",
    "idb": "^8.0.3",
    "lucide-react": "^0.483.0",
    "next": "16.1.1",
    "next-pwa": "^5.6.0",
    "next-themes": "^0.4.6",
    "react": "19.2.3",
    "react-day-picker": "^9.6.3",
    "react-dom": "19.2.3",
    "react-hook-form": "^7.55.0",
    "recharts": "^3.6.0",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.2.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.2",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.6.0",
    "@playwright/test": "^1.51.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.30",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.1",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "autoprefixer": "^10.4.23",
    "dotenv": "^16.5.0",
    "eslint": "^9.24.0",
    "eslint-config-next": "16.1.1",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-testing-library": "^7.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.5.0",
    "postcss": "^8.5.6",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "supabase": "^2.19.7",
    "tailwindcss": "^4.1.18",
    "ts-jest": "^29.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ],
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "keywords": [
    "finca",
    "bufalos",
    "ganaderia",
    "nextjs",
    "supabase",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tuusuario/finca-bufalos-datave.git"
  },
  "bugs": {
    "url": "https://github.com/tuusuario/finca-bufalos-datave/issues"
  },
  "homepage": "https://github.com/tuusuario/finca-bufalos-datave#readme"
}
